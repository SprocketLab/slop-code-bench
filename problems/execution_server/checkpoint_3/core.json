  "bz2_compression": {
    "expected": {
      "output": {
        "duration": 10,
        "exit_code": 0,
        "id": "UUID",
        "stderr": "",
        "stdout": "id,name\n1,test\n",
        "timed_out": false
      },
      "status_code": 201
    },
    "input": {
      "body": {
        "command": "bzcat data.csv.bz2",
        "env": {},
        "files": {
