{
  "version": 2,
  "operations": [
    {
      "type": "create_table",
      "table": "settings",
      "columns": [
        {
          "name": "id",
          "type": "INTEGER",
          "primary_key": true
        },
        {
          "name": "key",
          "type": "TEXT",
          "not_null": true
        },
        {
          "name": "value",
          "type": "TEXT",
          "default": "default_value"
        },
        {
          "name": "count",
          "type": "INTEGER",
          "default": "0"
        },
        {
          "name": "enabled",
          "type": "INTEGER",
          "default": "1"
        },
        {
          "name": "created_at",
          "type": "TIMESTAMP",
          "default": "CURRENT_TIMESTAMP"
        }
      ]
    }
  ],
  "description": "Create settings table with various default values"
}