{
  "arguments": "rules.json configs",
  "rules": {
    "add_logging_config": {
      "type": "merge_data",
      "glob": "**/*",
      "data": {
        "logging": {
          "level": "info",
          "handlers": [
            "console",
            "file"
          ]
        },
        "cache": {
          "enabled": true
        }
      }
    }
  }
}
