{
  "rules": {
    "a_rename_first": {
      "type": "rename_key",
      "old_key": "config.timeout",
      "new_key": "config.timeout_seconds"
    },
    "b_replace_renamed": {
      "type": "replace_value",
      "key": "config.timeout_seconds",
      "value": 30
    },
    "c_merge_config": {
      "type": "merge_data",
      "glob": "**/*",
      "data": {
        "config": {
          "timeout_seconds": 60,
          "retry_count": 5
        }
      }
    },
    "d_replace_again": {
      "type": "replace_value",
      "key": "config.timeout_seconds",
      "value": 120
    },
    "e_rename_chain": {
      "type": "rename_key",
      "old_key": "config.timeout_seconds",
      "new_key": "config.max_timeout"
    },
    "f_final_replace": {
      "type": "replace_value",
      "key": "config.max_timeout",
      "value": 180
    }
  },
  "files": {
    "configs/app.json": "{\"config\": {\"timeout\": 10, \"enabled\": true}}"
  }
}
