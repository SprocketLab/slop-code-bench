{
  "arguments": "rules.json configs",
  "rules": {
    "adjust_logging_handlers": {
      "type": "rename_key",
      "old_key": "logging.handlers",
      "new_key": "logging.outputs"
    },
    "increase_rollout": {
      "type": "replace_value",
      "key": "features.flags.rollout",
      "value": 0.4
    }
  }
}
