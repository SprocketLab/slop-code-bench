{
  "arguments": "rules.json configs",
  "rules": {
    "alpha_stringify_version": {
      "type": "replace_value",
      "key": "details.build.version",
      "value": "3"
    },
    "beta_template_version": {
      "type": "template_string",
      "key": "details.build.version",
      "variables": {
        "owner": "metadata.owner"
      },
      "template": "${owner}-build"
    }
  }
}
