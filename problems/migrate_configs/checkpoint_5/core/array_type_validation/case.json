{
  "arguments": "rules.json configs",
  "status_code": 1,
  "rules": {
    "validate_endpoint_types": {
      "type": "validate_type",
      "scope": "validation",
      "key": "services[*].port",
      "expected_type": "number"
    }
  },
  "input_files": [
    {
      "path": "configs/app.yaml",
      "file_type": "text",
      "content": "services:\n  - name: api\n    port: 8080\n  - name: worker\n    port: \\\"8081\\\"\n"
    }
  ]
}
