{
  "arguments": "rules.json configs",
  "status_code": 1,
  "rules": {
    "validate_service_name": {
      "type": "validate_value",
      "scope": "validation",
      "key": "service.name",
      "constraint": {
        "pattern": "^[a-z][a-z0-9-]*$"
      }
    }
  },
  "input_files": [
    {
      "path": "configs/app.yaml",
      "file_type": "text",
      "content": "service:\n  name: MyService123\n"
    }
  ]
}
