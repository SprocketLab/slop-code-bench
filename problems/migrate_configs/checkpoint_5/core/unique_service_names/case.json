{
  "arguments": "rules.json configs",
  "status_code": 1,
  "rules": {
    "unique_service_names": {
      "type": "unique_array_values",
      "scope": "validation",
      "array_path": "services",
      "field": "name"
    }
  },
  "input_files": [
    {
      "path": "configs/app.yaml",
      "file_type": "text",
      "content": "services:\n  - name: api\n    port: 8080\n  - name: worker\n    port: 8081\n  - name: api\n    port: 8082\n"
    }
  ]
}
