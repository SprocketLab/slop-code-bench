{
  "arguments": "rules.json configs",
  "status_code": 1,
  "rules": {
    "a_require_database": {
      "type": "require_keys",
      "scope": "validation",
      "keys": [
        "database.host",
        "database.port"
      ]
    },
    "b_check_timeout_type": {
      "type": "validate_type",
      "scope": "validation",
      "key": "timeout",
      "expected_type": "number"
    }
  },
  "input_files": [
    {
      "path": "configs/app.yaml",
      "file_type": "text",
      "content": "timeout: \\\"90\\\"\ndatabase:\n  host: prod.db.internal\n"
    }
  ]
}
